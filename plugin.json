{
  "id": "com.mattermost.plugin-expense-bot",
  "name": "Mattermost Expense Bot",
  "description": "Bot for mattermost that allows users to submit expense claims and track their status",
  "homepage_url": "https://github.com/nilsbrenkman/mattermost-plugin-expense-bot",
  "support_url": "https://github.com/nilsbrenkman/mattermost-plugin-expense-bot/issues",
  "icon_path": "assets/starter-template-icon.svg",
  "min_server_version": "6.2.1",
  "server": {
    "executables": {
      "linux-amd64": "server/dist/plugin-linux-amd64",
      "linux-arm64": "server/dist/plugin-linux-arm64",
      "darwin-amd64": "server/dist/plugin-darwin-amd64",
      "darwin-arm64": "server/dist/plugin-darwin-arm64",
      "windows-amd64": "server/dist/plugin-windows-amd64.exe"
    }
  },
  "settings_schema": {
    "header": "Expense Bot Settings",
    "footer": "",
    "settings": [
      {
        "key": "ChannelID",
        "display_name": "Posting Channel",
        "type": "text",
        "help_text": "Select the channel where all expense claims will be posted.",
        "placeholder": "Select a channel"
      }
    ]
  }
}
